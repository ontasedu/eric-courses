<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<query name="getEmployeesInSalaryRangeHql">
		<![CDATA[
			from Employee e where e.salary between ? and ? order by e.salary
		]]> 
	</query>

</hibernate-mapping>