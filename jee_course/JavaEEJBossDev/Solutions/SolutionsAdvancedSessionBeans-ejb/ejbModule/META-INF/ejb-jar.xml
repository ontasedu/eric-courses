<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:ejb="http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd" version="3.1">
  <enterprise-beans>
    <session>
      <ejb-name>AccountManagerBean</ejb-name>
      <env-entry>
        <env-entry-name>maxWithdrawalEntry</env-entry-name>
        <env-entry-type>java.lang.Double</env-entry-type>
        <env-entry-value>300</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>handlingFeeEntry</env-entry-name>
        <env-entry-type>java.lang.Double</env-entry-type>
        <env-entry-value>20</env-entry-value>
        <injection-target>
          <injection-target-class>solutions.advancedsessionbeans.ejb.AccountManagerBean</injection-target-class>
          <injection-target-name>handlingFee</injection-target-name>
        </injection-target>
      </env-entry>
    </session>
  </enterprise-beans>
  <ejb-client-jar>SolutionsAdvancedSessionBeans-ejbClient.jar</ejb-client-jar>
</ejb-jar>